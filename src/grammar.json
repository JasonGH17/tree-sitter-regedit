{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "regedit",
  "word": "value_type",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "header"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "section"
              },
              {
                "type": "SYMBOL",
                "name": "entry"
              }
            ]
          }
        }
      ]
    },
    "comment": {
      "type": "PATTERN",
      "value": ";[^\\n]*"
    },
    "header": {
      "type": "PATTERN",
      "value": "(?:REGEDIT4|Windows Registry Editor Version .*)"
    },
    "section": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "FIELD",
          "name": "path",
          "content": {
            "type": "SYMBOL",
            "name": "section_path"
          }
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "section_path": {
      "type": "PATTERN",
      "value": "[^\\]\\n]+"
    },
    "entry": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "value_name"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "value_data"
          }
        }
      ]
    },
    "value_name": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "STRING",
          "value": "@"
        }
      ]
    },
    "value_data": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "value_type"
            },
            {
              "type": "STRING",
              "value": ":"
            },
            {
              "type": "SYMBOL",
              "name": "value_body"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "number"
        }
      ]
    },
    "value_type": {
      "type": "PATTERN",
      "value": "(dword|qword|hex(?:\\([0-9a-fA-F]+\\))?)"
    },
    "value_body": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "hex_list"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        }
      ]
    },
    "hex_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "hex_byte"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "hex_byte"
              }
            ]
          }
        }
      ]
    },
    "hex_byte": {
      "type": "PATTERN",
      "value": "[0-9A-Fa-f]{1,2}"
    },
    "string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "escaped_char"
              },
              {
                "type": "PATTERN",
                "value": "[^\"\\\\]+"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "escaped_char": {
      "type": "PATTERN",
      "value": "\\\\."
    },
    "number": {
      "type": "PATTERN",
      "value": "0[xX][0-9a-fA-F]+|[0-9]+"
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "PATTERN",
      "value": "[ \\t\\r\\n]+"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}